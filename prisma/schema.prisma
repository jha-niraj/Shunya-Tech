generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Contact {
    id          String   @id @default(cuid())
    inquiryType String
    name        String
    email       String
    message     String   @db.Text
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    @@index([email])
}